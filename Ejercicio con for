using System;

class Program
{
    static void Main()
    {
        int sumaPares = 0;

        for (int i = 1; i <= 20; i++)
        {
            if (i % 2 == 0) 
            {
                sumaPares += i; 
            }
        }

        Console.WriteLine("La suma de los nÃºmeros pares del 1 al 20 es: " + sumaPares);
    }
}
